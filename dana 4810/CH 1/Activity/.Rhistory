# M08 Discrete RV & Distributions
# Discrete Uniform Distributions
myvector <- seq(1:6) # rolling a die
mytab <- proportions(table(myvector))
barplot(mytab,
main = "Discrete Uniform Distribution",
ylim = c(0,0.2),
col = "light blue")
# Bernoulli Distributions
myvector <- c(rep(0,8),rep(1,2))
mytab <- proportions(table(myvector))
barplot(mytab,
main = "Bernoulli Distribution with p=0.2",
col = "light blue")
# Binomial Probability Calculations
