plot(CorrData)
summary(varGPA)
summary(varStudy)
plot(varStudy, # X-variable
varGPA, # Y-variable
main = "Scatterplot of GPA vs. Study",
ylab = "GPA",
xlab = "Study Time (in hrs)",
